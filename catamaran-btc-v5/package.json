{
  "name": "catamaran-btc",
  "version": "1.0.0",
  "description": "swap btc for stacks assets",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "vitest run",
    "watch": "vitest watch",
    "watch:clar": "npx clarigen --watch",
    "test:report": "vitest run -- --coverage --costs",
    "test:watch": "chokidar \"tests/**/*.ts\" \"contracts/**/*.clar\" -c \"npm run test:report\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/friedger/clarity-catamaranswaps.git"
  },
  "keywords": [
    "bitcoin",
    "stacks",
    "clarity",
    "swaps",
    "layer1",
    "layer2"
  ],
  "author": "friedger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/friedger/clarity-catamaranswaps/issues"
  },
  "homepage": "https://github.com/friedger/clarity-catamaranswaps#readme",
  "dependencies": {
    "@clarigen/core": "^2.0.6",
    "@clarigen/test": "^2.0.6",
    "@hirosystems/clarinet-sdk": "^2.6.0",
    "@stacks/transactions": "^6.15.0",
    "chokidar-cli": "^3.0.0",
    "prettier": "^3.3.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vitest-environment-clarinet": "^2.0.0"
  },
